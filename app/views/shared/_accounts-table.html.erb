<table class="table table-hover dash-table">
  <thead>
    <tr>
      <th scope="col" class= "logo-accounts">Bank Logo</th>
      <th scope="col">Account Name</th>
    </tr>
  </thead>
  <tbody>
    <% @account_names.each do |account_name|  %>
      <tr>
        <td class="logo-accounts">Logo</td>
        <td class="accounts"><%= account_name %></td>
      </tr>
    <% end %>
  </tbody>
</table>
